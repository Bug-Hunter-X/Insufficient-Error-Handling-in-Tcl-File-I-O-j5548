proc read_file {filename} {    set fp [open $filename r]    if {$fp == -1} {        puts stderr "Error opening file: $filename"        return -1    }    set content [read $fp]    close $fp    return $content}proc write_file {filename content} {    set fp [open $filename w]    if {$fp == -1} {        puts stderr "Error opening file: $filename for writing"        return -1    }    puts -nonewline $fp $content    close $fp    return 1}set file_content "This is the content of the file."\nwrite_file myfile.txt $file_content\nset result [read_file myfile.txt]\nif {$result == -1} {    puts "Error reading file." } else {    puts "Content of file: $result"}file delete myfile.txt