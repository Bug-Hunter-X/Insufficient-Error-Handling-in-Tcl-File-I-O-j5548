proc read_file {filename} {    set fp [open $filename r]    if {$fp == -1} {        return -1    }    set content [read $fp]    close $fp    return $content}proc write_file {filename content} {    set fp [open $filename w]    if {$fp == -1} {        return -1    }    puts $fp $content    close $fp    return 1}set file_content "This is the content of the file."\nwrite_file myfile.txt $file_content\nset result [read_file myfile.txt]\nputs "Content of file: $result"